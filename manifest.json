{
	"name": "Wiki Redirect",
	"description": "Redirect to mobile page of Wikipedia",
	"version": "1.0",
	"declarative_net_request": {
		"rule_resources": [{
		"id": "ruleset_1",
		"enabled": true,
		"path": "rules.json"
		}]
	 },
	"icons": {
		"16": "img/icon-16.png",
		"48": "img/icon-48.png",
		"128": "img/icon-128.png"
	},
	"manifest_version": 3,
	"permissions": [
		"declarativeNetRequest"
	],
	"host_permissions": [
		"*://*.wikipedia.org/"
	]
}